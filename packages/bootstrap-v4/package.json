{
  "name": "@aurelia-toolbelt/bootstrap-v4",
  "version": "0.1.0",
  "description": "Aurelia toolbelt component set.",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurelia-toolbelt/vNext"
  },
  "license": "MIT",
  "dependencies": {
    "aurelia": "dev",
    "@aurelia-toolbelt/bootstrap-v4-core": "0.1.0",
    "@aurelia-toolbelt/custom-attributes": "0.1.0",
    "bootstrap": "4.5.0",
    "jquery": "3.5.1"
  },
  "devDependencies": {
    "@types/bootstrap": "4.5.0",
    "@types/jquery": "3.3.38",
    "@aurelia/testing": "dev",
    "htmlhint": "^0.11.0",
    "sass-lint": "^1.0.0",
    "browser-do": "^2.0.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.0.0",
    "html-webpack-plugin": "^3.0.0",
    "webpack-bundle-analyzer": "^3.0.0",
    "url-loader": "^4.0.0",
    "file-loader": "^6.0.0",
    "to-string-loader": "^1.0.0",
    "style-loader": "^1.0.0",
    "css-loader": "^3.0.0",
    "@aurelia/webpack-loader": "dev",
    "ts-loader": "^6.0.0",
    "sass-loader": "^8.0.0",
    "sass": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "autoprefixer": "^9.0.0",
    "webpack-shell-plugin-next": "^1.0.0",
    "@types/node": "^13.0.0",
    "typescript": "^3.0.0",
    "tslint": "^6.0.0",
    "mocha": "^7.0.0",
    "chai": "^4.0.0",
    "@types/mocha": "^7.0.0"
  },
  "scripts": {
    "lint:css": "sass-lint -c .sass-lint.yml \"src/**/*.scss\"",
    "lint:html": "htmlhint -c .htmlhintrc src",
    "lint": "npm run lint:js && npm run lint:html && npm run lint:css",
    "test": "npm run test:headless",
    "pretest": "npm run lint && npm run build:test",
    "start": "webpack-dev-server",
    "build": "rimraf dist && webpack --env production",
    "analyze": "rimraf dist && webpack --env production --analyze",
    "build:test": "rimraf dist && webpack --env test",
    "test:watch": "rimraf dist && webpack --env test --watch --runTest",
    "lint:js": "tslint --project tsconfig.json",
    "test:headless": "browser-do --mocha < dist/entry-bundle.js",
    "test:chrome": "browser-do --mocha --browser chrome < dist/entry-bundle.js"
  },
  "publishConfig": {
    "access": "public"
  }
}