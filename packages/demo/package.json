  "dependencies": {
    "aurelia": "dev",
    "@aurelia-toolbelt/bootstrap-v5": "0.1.0",
    "@aurelia-toolbelt/bootstrap-v5-plus": "0.1.0",
    "@aurelia-toolbelt/custom-attributes": "0.1.0"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "@aurelia/testing": "dev",
    "htmlhint": "^0.14.2",
    "browser-do": "^3.0.2",
    "rimraf": "^3.0.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "html-webpack-plugin": "^5.1.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "url-loader": "^4.1.1",
    "file-loader": "^6.2.0",
    "style-loader": "^2.0.0",
    "css-loader": "^5.0.1",
    "@aurelia/webpack-loader": "dev",
    "ts-loader": "^8.0.14",
    "postcss-loader": "^4.1.0",
    "postcss": "^8.2.10",
    "autoprefixer": "^10.2.1",
    "webpack-shell-plugin-next": "^2.1.1",
    "@types/node": "^14.14.21",
    "typescript": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "mocha": "^8.2.1",
    "chai": "^4.2.0",
    "@types/mocha": "^8.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^6.2.1",
    "eslint-plugin-cypress": "^2.11.2",
    "cypress-shadow-dom": "^1.4.1",
    "start-server-and-test": "^1.11.7"
  },
  "scripts": {
    "lint:js": "eslint src test cypress --ext .js,.ts",
    "lint:html": "htmlhint -c .htmlhintrc src",
    "lint": "npm run lint:js && npm run lint:html",
    "test": "npm run test:headless",
    "pretest": "npm run lint && npm run build:test",
    "start": "webpack serve",
    "build": "rimraf dist && webpack --env production",
    "analyze": "rimraf dist && webpack --env production --analyze",
    "build:test": "rimraf dist && webpack --env test",
    "test:watch": "rimraf dist && webpack --env test --watch --runTest",
    "test:headless": "browser-do --mocha < dist/entry.bundle.js",
    "test:chrome": "browser-do --mocha --browser chrome < dist/entry.bundle.js",
    "cypress": "cypress run",
    "test:e2e": "cross-env CI=1 start-server-and-test start http://localhost:9000 cypress"
  }
}